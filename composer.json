{
        "name": "atk14/recaptcha-field",
        "type": "library",
        "description": "A field for protecting forms against spam in ATK14 applications",
        "keywords": ["captcha", "recaptcha", "field", "forms", "atk14"],
        "homepage": "https://github.com/atk14/RecaptchaField",
        "license": ["MIT"],
        "authors": [
                {
                        "name": "Jaromir Tomek",
                        "homepage": "https://github.com/yarri"
                }
        ],
        "require": {
                "php": ">=5.3.0"
        },
        "require-dev": {
                "atk14/tester": "*",
                "atk14/forms": "dev-master",
                "atk14/core": "dev-master",
                "atk14/http": "^0.3.0"
        },
        "autoload": {
                "classmap": ["src/app/fields","src/app/widgets"]
        }
}
